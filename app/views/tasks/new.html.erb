<%
  @page_actions = []
  
  if ProjectTaskList.can_be_created_by(@logged_user, @active_project)
    @page_actions << {:title => :add_task_list, :url=> new_task_list_path}
  end
%>
<% form_tag new_task_path(:task_list_id => @task_list.id) do %>
  <%= render :partial => 'form', :object => @task %>
  <div><button class="submit" type="submit"><%= :add_task.l %></button></div>
<% end %>
