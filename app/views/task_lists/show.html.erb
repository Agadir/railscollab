<%
  @page_title = h @task_list.name
  @bread_crumbs = project_crumbs(@task_list.name, [{:title => :tasks, :url => task_lists_path}])
  
  @page_actions = []
  
  if ProjectTaskList.can_be_created_by(@logged_user, @active_project)
    @page_actions << {:title => :add_task_list, :url=> new_task_list_path}
  end
  
  @additional_stylesheets = ['project/task_list']
%>
<%= render :partial => 'show', :object => @task_list, :locals => {:on_list_page => true} %>
