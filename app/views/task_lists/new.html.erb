<%
  @page_title = :add_task_list.l
  @bread_crumbs = project_crumbs(:add_task_list, [{:title => 'Tasks', :url => task_lists_path}])
  
  @page_actions = []
  
  if ProjectTaskList.can_be_created_by(@logged_user, @active_project)
    @page_actions << {:title => :add_task_list, :url=> new_task_list_path}
  end
  
  @additional_stylesheets = ['project/task_list']
%>
<%= form_tag task_lists_path %>
<%= render :partial => 'form' %>
    <button class="submit" type="submit"><%= :add_task_list.l %></button>
</form>
