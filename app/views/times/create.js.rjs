if @time.done_date.nil?
  # Add to running time list
  page.call "Project.updateRunningTimes", @running_times.size, t('running_times', :count => @running_times.size)
  page.select('#running_times_menu ul').append(render({:partial => 'listed', :collection => [@time]}))
  
  # Update task list item
  unless @time.task.nil?
    page.replace("task_item_#{@time.task.id}", :partial => 'tasks/show', :collection => [@time.task] )
  end
end